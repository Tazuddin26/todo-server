{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ],
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "https://todo-server-rho-bice.vercel.app/socket.io/$1"
    }
  ]
}
